{
  "name": "@webmcp/helpers",
  "version": "0.0.1",
  "description": "Lightweight helper utilities for WebMCP userscript development - DOM waiting, text input, response helpers",
  "keywords": [
    "webmcp",
    "mcp",
    "model-context-protocol",
    "dom-helpers",
    "userscript",
    "browser-automation",
    "ai-tools"
  ],
  "homepage": "https://github.com/WebMCP-org/npm-packages/tree/main/webmcp-helpers#readme",
  "bugs": {
    "url": "https://github.com/WebMCP-org/npm-packages/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WebMCP-org/npm-packages.git",
    "directory": "packages/webmcp-helpers"
  },
  "license": "MIT",
  "author": "WebMCP Contributors",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "check": "biome check --write .",
    "clean": "rm -rf dist .turbo",
    "dev": "tsdown --watch",
    "format": "biome format --write .",
    "lint": "biome lint --write .",
    "prepublishOnly": "node ../../scripts/validate-publish.js && pnpm run build",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@types/node": "catalog:",
    "tsdown": "catalog:",
    "typescript": "catalog:"
  },
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
